{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dog-tracker": {
      "root": "",
      "sourceRoot": "cleint/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "less"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/dog-tracker/client",
            "index": "cleint/src/index.html",
            "main": "cleint/src/main.ts",
            "polyfills": "cleint/src/polyfills.ts",
            "tsConfig": "cleint/src/tsconfig.app.json",
            "assets": [
              "cleint/src/favicon.ico",
              "cleint/src/assets"
            ],
            "styles": [
              "cleint/src/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "cleint/src/environments/environment.ts",
                  "with": "cleint/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dog-tracker:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dog-tracker:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dog-tracker:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "cleint/src/test.ts",
            "polyfills": "cleint/src/polyfills.ts",
            "tsConfig": "cleint/src/tsconfig.spec.json",
            "karmaConfig": "cleint/src/karma.conf.js",
            "styles": [
              "cleint/src/styles.less"
            ],
            "scripts": [],
            "assets": [
              "cleint/src/favicon.ico",
              "cleint/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "cleint/src/tsconfig.app.json",
              "cleint/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dog-tracker-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "dog-tracker:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "dog-tracker"
}